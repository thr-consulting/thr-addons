{
	"name": "@thx/controls",
	"version": "11.4.0",
	"description": "A collection of components designed with SemanticUI.",
	"repository": {
		"type": "git",
		"url": "https://github.com/thr-consulting/thr-addons.git"
	},
	"license": "MIT",
	"author": "Mike Kornelson <darkadept@durbn.net>",
	"engines": {
		"node": ">=12"
	},
	"bugs": {
		"url": "https://github.com/thr-consulting/thr-addons/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"index.js"
	],
	"scripts": {
		"Xbuild": "yarn build:dev && yarn build:prod",
		"Xbuild:dev": "NODE_ENV=development webpack --config webpack.js",
		"Xbuild:prod": "NODE_ENV=production webpack --config webpack.js",
		"Xlint": "eslint --cache --ext js,ts src",
		"Xlint:fix": "eslint --cache --fix --ext js,ts src",
		"Xts": "tsc",
		"Xyalc:publish": "yalc publish --push",
		"Xprepublish": "yarn build && yarn ts",
		"Xstorybook": "start-storybook"
	},
	"devDependencies": {
		"webpack-cli": "^3.3.9"
	},
	"peerDependencies": {
		"react": "^16.x",
		"react-dom": "^16.x",
		"semantic-ui-react": "^0.x"
	},
	"dependencies": {
		"debug": "^4.1.1",
		"lodash": "^4.17.15",
		"formik": "^2.1.2",
		"flat": "^5.0.0",
		"yup": "^0.28.0"
	}
}
