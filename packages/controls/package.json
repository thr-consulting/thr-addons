{
  "name": "@thx/controls",
  "version": "7.1.6",
  "description": "A collection of components designed with SemanticUI.",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "controls",
    "semantic-ui"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run clean:dist && npm run build:lib && npm run build:dist && npm run assets:dist",
    "build:lib": "babel src --out-dir lib",
    "build:dist": "NODE_ENV=production webpack",
    "clean:lib": "rimraf ./lib/*",
    "clean:dist": "rimraf ./dist/*",
    "assets:dist": "rimraf ./dist/*.js && cpy dist/* lib/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json --source \" \" --separators -g table --plugin dmd-clear > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "docs:storybook": "build-storybook -c .storybook -o docs/storybook"
  },
  "devDependencies": {
    "babel-loader": "^8.0.2",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "cpy-cli": "^2.0.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "identity-obj-proxy": "^3.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-formal": "0.25.4",
    "react-test-renderer": "^16.4.0",
    "rimraf": "^2.6.2",
    "semantic-ui-react": "^0.81.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-formal": "0.25.4",
    "semantic-ui-react": "^0.x"
  },
  "dependencies": {
    "@thx/money": "^7.1.7",
    "@thx/tproptypes": "^7.1.1",
    "@thx/date": "^7.1.3",
    "@thx/tschemas": "^7.1.1",
    "debug": "^3.1.0",
    "formik": "^0.11.11",
    "inputmask": "^4.0.0",
    "js-money": "^0.6.3",
    "js-joda": "^1.6.2",
    "moment": "^2.22.2",
    "prop-types": "^15.6.0",
    "react-datepicker": "^1.5.0",
    "react-widgets": "^4.3.2",
    "react-widgets-moment": "^4.0.15",
    "react-input-message": "^0.15.1",
    "react-responsive": "^4.0.4",
    "react-text-mask": "^5.4.1",
    "text-mask-addons": "^3.7.2",
    "string-hash": "^1.1.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
