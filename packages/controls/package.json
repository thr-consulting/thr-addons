{
  "name": "@thx/controls",
  "version": "11.2.1",
  "description": "A collection of components designed with SemanticUI.",
  "main": "lib/controls.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "controls",
    "semantic-ui"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "NODE_ENV=production webpack",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "../../tools/coverage.sh controls",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh controls",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "docs:storybook": "../../tools/storybook.sh controls"
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x",
    "semantic-ui-react": "^0.x"
  },
  "dependencies": {
    "@thx/date": "^10.3.0",
    "@thx/money": "^10.1.1",
    "@thx/tschemas": "^10.1.1",
    "debug": "^4.1.1",
    "formik": "^1.5.4",
    "inputmask": "^4.0.8",
    "js-joda": "^1.9.3",
    "js-money": "^0.6.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "number-to-date-month-name": "^1.0.2",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.5.0",
    "react-input-message": "^0.15.1",
    "react-responsive": "^6.1.1",
    "react-text-mask": "^5.4.1",
    "string-hash": "^1.1.3",
    "text-mask-addons": "^3.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-storyshots": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@types/react": "^16.8.14",
    "mini-css-extract-plugin": "^0.8.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  },
  "gitHead": "7e4c5f587db721caec9e5945f7b5ca135a18dae6"
}
