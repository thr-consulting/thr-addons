{
	"presets": [
		"react",
		"flow",
		[
			"env",
			{
				"modules": "commonjs",
				"targets": {
					"browsers": [
						"> 1%",
						"last 4 versions",
						"not ie < 9"
					]
				}
			}
		]
	],
	"plugins": [
		"transform-class-properties",
		"transform-object-rest-spread"
	],
	"env": {
		"development": {
			"plugins": [
				"flow-react-proptypes"
			]
		},
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	}
}
