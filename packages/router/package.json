{
  "name": "@thx/router",
  "version": "10.2.0",
  "description": "A collection of components for router and layout",
  "main": "lib/router.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "layout",
    "router"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "NODE_ENV=production webpack",
    "clean:lib": "rimraf ./lib/*",
    "TURNEDOFFtest": "jest",
    "TURNEDOFFtest:watch": "jest --watch",
    "TURNEDOFFtest:coverage": "../../tools/coverage.sh router",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh router",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x",
    "react-router-dom": "^5.x",
    "semantic-ui-react": "^0.x"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "query-string": "^6.4.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
