{
  "name": "@thx/tproptypes",
  "version": "3.0.3",
  "description": "Useful React PropType shortcuts",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "proptypes"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -m grouped -g grouped -c jsdoc.json -f src/*.js > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json"
  },
  "devDependencies": {
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "js-joda": "^1.6.2",
    "js-money": "^0.6.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
