{
  "name": "@thx/tproptypes",
  "version": "8.0.0",
  "description": "Useful React PropType shortcuts",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "proptypes"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "NODE_ENV=development jsdoc2md -c jsdoc.json --source \" \" --separators -g table --plugin dmd-clear > docs/README.md",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "dependencies": {
    "js-joda": "^1.9.2",
    "js-money": "^0.6.3",
    "prop-types": "^15.6.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
