{
  "name": "@thx/date",
  "version": "10.2.0",
  "description": "Date utils",
  "main": "lib/date.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "date",
    "moment"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "NODE_ENV=production webpack",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "../../tools/coverage.sh date",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh date",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "peerDependencies": {
    "graphql": "^14.x"
  },
  "dependencies": {
    "@js-joda/locale_en-us": "^2.0.1",
    "debug": "^4.1.1",
    "js-joda": "^1.9.3",
    "js-joda-timezone": "^2.0.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "jest-date-mock": "^1.0.7",
    "rimraf": "^2.6.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    }
  }
}
