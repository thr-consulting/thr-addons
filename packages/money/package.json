{
  "name": "@thx/money",
  "version": "3.0.1",
  "description": "Money addons",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "controls",
    "money",
    "currency",
    "js-money"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "babel src --out-dir lib && babel index.server.js --out-file server.js --auxiliary-comment-before=eslint-disable",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json -f src/*.js > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-jason": "^3.1.0",
    "coverbadge": "^0.5.5",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "^0.57.3",
    "ink-docstrap": "^1.3.0",
    "jest": "^21.1.0",
    "jsdoc": "^3.5.4",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "rimraf": "^2.6.2",
    "semantic-ui-react": "^0.75.1"
  },
  "peerDependencies": {
    "react": "^15.6.0",
    "semantic-ui-react": "^0.75.1"
  },
  "dependencies": {
    "@thx/tproptypes": "^3.0.0",
    "accounting": "^0.4.1",
    "debug": "^3.0.0",
    "inputmask": "^3.3.7",
    "js-money": "^0.6.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
