{
  "name": "@thx/money",
  "version": "3.0.3",
  "description": "Money addons",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "controls",
    "money",
    "currency",
    "js-money"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "babel src --out-dir lib && babel index.server.js --out-file server.js --auxiliary-comment-before=eslint-disable",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json -f src/*.js > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "semantic-ui-react": "^0.78.2"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "semantic-ui-react": "^0.78.0"
  },
  "dependencies": {
    "@thx/tproptypes": "^3.0.3",
    "accounting": "^0.4.1",
    "debug": "^3.1.0",
    "inputmask": "^3.3.11",
    "js-money": "^0.6.3",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
