{
  "name": "@thx/ducks",
  "version": "3.0.0",
  "description": "Helpers for Redux ducks layout",
  "main": "lib/index.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": ["redux", "ducks"],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json -f src/*.js > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-jason": "^3.1.0",
    "coverbadge": "^0.5.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.54.1",
    "ink-docstrap": "^1.3.0",
    "jest": "^21.1.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "rimraf": "^2.6.2"
  }
}
