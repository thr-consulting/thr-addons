{
  "name": "@thx/money-ui",
  "version": "7.1.2",
  "description": "Money controls",
  "main": "lib/index.js",
  "private": false,
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "react",
    "controls",
    "money",
    "currency",
    "js-money"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "babel src --out-dir lib",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && cat coverage/lcov.info | coverbadge -o coverage/coverage.svg",
    "flow": "flow",
    "lint": "eslint *.js src",
    "docs": "jsdoc -c jsdoc.json",
    "docs:md": "jsdoc2md -c jsdoc.json --source \" \" --separators -g table --plugin dmd-clear > docs/README.md",
    "docs:explain": "jsdoc -X -c jsdoc.json",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@storybook/addon-console": "^1.0.4",
    "@storybook/react": "^3.4.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-test-renderer": "^16.4.0",
    "rimraf": "^2.6.2",
    "semantic-ui-react": "^0.81.1"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "semantic-ui-react": "^0.x"
  },
  "dependencies": {
    "@thx/tproptypes": "^7.1.1",
    "debug": "^3.1.0",
    "inputmask": "^3.3.11",
    "js-money": "^0.6.3",
    "prop-types": "^15.6.0",
    "react-text-mask": "^5.4.1",
    "text-mask-addons": "^3.7.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
