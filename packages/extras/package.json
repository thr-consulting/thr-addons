{
  "name": "@thx/extras",
  "version": "10.0.0",
  "description": "Extra utils",
  "main": "lib/extras.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "extra",
    "utilities"
  ],
  "files": [
    "client.js",
    "server.js",
    "mongooseTypes.js"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run build:server && npm run build:mongoosetypes && npm run build:client",
    "build:server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "build:mongoosetypes": "NODE_ENV=production webpack --config webpack.server.mongoose.config.js",
    "build:client": "NODE_ENV=production webpack --config webpack.client.config.js",
    "clean": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "../../tools/coverage.sh extras",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "ts": "tsc",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh extras",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "peerDependencies": {
    "graphql": "^14.x",
    "mongoose": "^5.x",
    "react": "^16.x",
    "react-apollo": "^2.x"
  },
  "dependencies": {
    "@thx/date": "^10.1.1",
    "@thx/money": "^10.1.1",
    "colors": "^1.3.3",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "fast-file-rotate": "^1.0.1",
    "ioredis": "^4.9.3",
    "js-joda": "^1.10.1",
    "lodash": "^4.17.11",
    "winston": "^3.2.1",
    "winston-gelf": "^1.7.0"
  },
  "devDependencies": {
    "@types/mongoose": "^5.3.26",
    "rimraf": "^2.6.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    }
  }
}
