{
  "name": "@thx/extras",
  "version": "10.5.0",
  "description": "Extra utils",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "extra",
    "utilities"
  ],
  "files": [
    "client.js",
    "server.js",
    "mongooseTypes.js"
  ],
  "scripts": {
    "prepublish": "yarn run build",
    "build": "yarn clean && yarn build:client && yarn build:server && yarn build:mongoosetypes",
    "build:client": "NODE_ENV=production webpack --config webpack/client.js && NODE_ENV=development webpack --config webpack/client.js",
    "build:server": "NODE_ENV=production webpack --config webpack/server.js && NODE_ENV=development webpack --config webpack/server.js",
    "build:mongoosetypes": "NODE_ENV=production webpack --config webpack/mongoosetypes.js && NODE_ENV=development webpack --config webpack/mongoosetypes.js",
    "clean": "rimraf lib",
    "DISABLEDtest": "jest",
    "DISABLEDtest:watch": "jest --watch",
    "DISABLEDtest:coverage": "../../tools/coverage.sh extras",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "ts": "tsc",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh extras",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "dependencies": {
    "@thx/date": "^10.3.0",
    "@thx/money": "^10.1.1",
    "aws-sdk": "^2.507.0",
    "colors": "^1.3.3",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "delete-empty": "^3.0.0",
    "fast-file-rotate": "^1.0.1",
    "file-type": "^11.1.0",
    "js-joda": "^1.10.1",
    "lodash": "^4.17.11",
    "memfs": "^3.0.1",
    "mime-types": "^2.1.24",
    "mongoose": "^5.7.6",
    "winston": "^3.2.1",
    "winston-gelf": "^1.7.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@types/mongoose": "^5.5.23",
    "apollo-client": "^2.5.1",
    "babel-jest": "^24.5.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "graphql": "^14.1.1",
    "jest": "^24.7.1",
    "jsdoc-to-markdown": "^5.0.0",
    "lerna": "^3.13.4",
    "mongodb": "^3.2.1",
    "react": "^16.8.4",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "redis": "^2.8.0",
    "rimraf": "^2.6.3",
    "semantic-ui-react": "^0.87.1",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.9"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    }
  },
  "gitHead": "7e4c5f587db721caec9e5945f7b5ca135a18dae6"
}
