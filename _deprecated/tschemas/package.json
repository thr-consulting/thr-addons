{
  "name": "@thx/tschemas",
  "version": "10.1.1",
  "description": "Useful yup schema shortcuts",
  "main": "lib/tschemas.js",
  "author": "Mike Kornelson <mike@durbn.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thr-consulting/thr-addons.git"
  },
  "keywords": [
    "yup",
    "schema"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean:lib && npm run build:lib",
    "build:lib": "NODE_ENV=production webpack",
    "clean:lib": "rimraf ./lib/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "../../tools/coverage.sh tschemas",
    "lint": "eslint --cache --ext js,ts,tsx src",
    "docs": "NODE_ENV=development jsdoc -c jsdoc.json",
    "docs:md": "../../tools/jsdocmd.sh tschemas",
    "docs:explain": "NODE_ENV=development jsdoc -X -c jsdoc.json"
  },
  "dependencies": {
    "escape-string-regexp": "^2.0.0",
    "js-joda": "^1.9.3",
    "js-money": "^0.6.3",
    "moment": "^2.24.0",
    "social-insurance-number": "^0.0.4",
    "yup": "^0.27.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/../../tests/setupTests.js"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
    }
  }
}
